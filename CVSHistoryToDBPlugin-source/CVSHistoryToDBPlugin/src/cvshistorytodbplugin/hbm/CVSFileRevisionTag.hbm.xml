<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="cvshistorytodbplugin.model.CVSFileRevisionTag" table="file_revision_tag">

        <id name="fileRevisionTagId" type="java.lang.Integer" column="file_revision_tag_id">
            <generator class="identity"/>
        </id>
        
        <many-to-one name="fileRevision" class="cvshistorytodbplugin.model.CVSFileRevision" column="file_revision_id" />      
		<many-to-one name="tag" class="cvshistorytodbplugin.model.CVSTag" column="tag_id" />

    </class>
    
</hibernate-mapping>